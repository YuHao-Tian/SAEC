python - <<'PY'
import os, json, glob
def paths(js): 
    s=set()
    with open(js,'r',encoding='utf-8') as f:
        for ln in f:
            j=json.loads(ln)
            if 'image' in j: s.add(os.path.realpath(j['image']))
    return s
sft = paths('/home/vipuser/data/mvtec_qwen_sft/train.jsonl') | paths('/home/vipuser/data/mvtec_qwen_sft/val.jsonl')
hold = {os.path.realpath(p) for p in glob.glob('/home/vipuser/data/mvtec_cls_holdout1k_v1/val/*/*')}
print("OVERLAP =", len(sft & hold))
PY
